+.TH AMULEWEB 1 "August 2008" "aMule webserver v2.2.2" "aMule utilities"
 .SH NAME
 amuleweb \- aMule web server
 .SH SYNOPSIS
 .B amuleweb
 .RB [ \-h " " \fI<host> ]
 .RB [ \-p " " \fI<port> ]
-.RB [ \-P " " \fI<pass> ]
+.RB [ \-P " " \fI<passwd> ]
 .RB [ \-f " " \fI<path> ]
 .RB [ \-q ]
 .RB [ \-v ]
@@ -13,6 +13,8 @@
 .RB [ \-w ]
